{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string"
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1.0
        },
        "company_handle": {
            "type": "string"
        }
    }
}